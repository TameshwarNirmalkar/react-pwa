    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "react-scripts": "3.3.0",
    "react-snapshot": "^1.3.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
