{
  "name": "tamesh-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:start": "rm -rf /build && react-scripts build",
    "live": "node server",
    "start:env": "./node_modules/.bin/env-cmd -e development -r ./config/.env",
    "exp": "set NODE_ENV=development && ./node_modules/.bin/env-cmd -e development .env",
    "start:dev": "./node_modules/.bin/env-cmd -f ./config/.env.dev react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.8.2",
    "@material-ui/lab": "^4.0.0-alpha.38",
    "@material-ui/pickers": "^3.2.8",
    "bootstrap-scss": "4.4.1",
    "connected-react-router": "^6.6.1",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-datepicker": "^2.10.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^4.7.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-form": "^8.2.6",
    "redux-form-material-ui": "^4.3.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "node-sass": "^4.13.0",
    "react-scripts": "3.3.0",
    "react-snapshot": "^1.3.0",
    "redux-logger": "^3.0.6"
  }
}
